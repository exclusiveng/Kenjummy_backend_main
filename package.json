{
  "name": "@Kenjummy_backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "typeorm:ts": "typeorm-ts-node-commonjs -d src/config/migration.datasource.ts",
    "typeorm:js": "node ./node_modules/typeorm/cli.js -d dist/config/migration.datasource.js",
    "migration:generate": "npm run typeorm:ts -- migration:generate",
    "migration:run": "npm run typeorm:ts -- migration:run",
    "migration:revert": "npm run typeorm:ts -- migration:revert",
    "release": "npm run typeorm:js -- migration:run"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.17",
    "winston": "^3.18.3",
    "xss-clean": "^0.1.4",
    "zod": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.7",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.10",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
